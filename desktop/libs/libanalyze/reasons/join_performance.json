{
  "type": "SQLOperator",
  "node_name": "HASH_JOIN_NODE",
  "metric_names": [
    "ProbeRows",
    "ProbeTime"
  ],
  "rule": {
    "expr": "vars['ProbeTime'] - float(vars['ProbeRows']) / 0.005",
    "message": "Excess time (over expected time) spent in the hash join",
    "label": "Slow Hash Join"
  },
  "fix": {
    "fixable": false
  }
}