{
  "type": "SQLOperator",
  "node_name": "HDFS_SCAN_NODE",
  "metric_names": [
    "BytesRead"
  ],
  "exprs": [
    "max",
    "avg"
  ],
  "unit": 5,
  "rule": {
    "condition": "vars['IOBound']==True",
    "expr": "(vars['max'] - vars['avg']) / 100000000 / 5",
    "message": "Addition IO time cost by the skew (assuming 5 disks)",
    "label": "Bytes Read Skew",
    "prio": 2
  },
  "fix": {
    "fixable": false
  }
}