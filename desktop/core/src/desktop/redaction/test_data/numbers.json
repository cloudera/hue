      "replace": "#"
    },
    {
      "description": "No 8's",
      "trigger": "8",
      "search": "8",
      "replace": "#"
    },
    {
      "description": "No 9's",
      "trigger": "9",
      "search": "9",
      "replace": "#"
    }
  ]
}
